{
    "Name": "ToneAPI.pulse",
    "Description": "Displays Tone API kill data in the Stats tab.",
    "Version": "1.2.1",
    "LoadPriority": 1,
    "ConVars": [
        {
            "Name": "ToneURL",
            "DefaultValue": "https://tone.sleepycat.date/v2/client"
        },
        {
            "Name": "VersionURL",
            "DefaultValue": "https://raw.githubusercontent.com/ToneAPI/pulse/main/version.json"
        },
        {
            "Name": "MajorVersion",
            "DefaultValue": "1"
        },
        {
            "Name": "MinorVersion",
            "DefaultValue": "2"
        },
        {
            "Name": "PatchVersion",
            "DefaultValue": "1"
        }
    ],
    "Scripts": [
        {
        "Path": "pulse.gnut",
        "RunOn": "UI",
        "UICallback": {
            "After": "pulseInit"
        }
        }
    ]
}
